<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.2.0 https://maven.apache.org/xsd/archetype-descriptor-1.2.0.xsd" name="example"
    xmlns="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.2.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <requiredProperties>
    <requiredProperty key="version">
        <defaultValue>0.0.0-SNAPSHOT</defaultValue>
    </requiredProperty>
  </requiredProperties>
  <fileSets>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>github</directory>
      <includes>
        <include>**/*.json</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>github/workflows</directory>
      <includes>
        <include>**/*.yml</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <includes>
        <include>README.md</include>
        <include>gitignore</include>
      </includes>
    </fileSet>
  </fileSets>
  <modules>
    <module id="${rootArtifactId}.core" dir="core" name="${rootArtifactId}.core">
      <fileSets>
        <fileSet filtered="true" packaged="true" encoding="UTF-8">
          <directory>src/main/java</directory>
          <includes>
            <include>**/*.java</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="${rootArtifactId}.app" dir="app" name="${rootArtifactId}.app">
      <fileSets>
        <fileSet filtered="true" encoding="UTF-8">
          <includes>
            <include>descriptor.json</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="${rootArtifactId}.sdk" dir="sdk" name="${rootArtifactId}.sdk" />
  </modules>
</archetype-descriptor>
